{
  "name": "react-drip-form-material-ui",
  "version": "0.0.1-alpha.1",
  "description": "Material-UI component set for react-drip-form.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src/ -d lib/ --ignore \"**/__tests__/*\"",
    "test": "npm run test:lint && npm run test:unit",
    "test:watch": "npm run test:unit -- --watch",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:unit": "jest",
    "test:coverage": "npm run test:unit -- --coverage",
    "codecov": "npm run test:coverage && codecov",
    "demo": "webpack-dev-server --config demo/webpack.config.js",
    "demo:build": "cross-env NODE_ENV=production webpack -p --config demo/webpack.config.production.js",
    "demo:deploy": "npm run demo:build && gh-pages -d demo",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsuyoshiwada/react-drip-form-material-ui.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "form",
    "drip-form"
  ],
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/react-drip-form-material-ui/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/react-drip-form-material-ui#readme",
  "files": [
    "lib",
    "test"
  ],
  "peerDependencies": {
    "material-ui": ">=0.16.0",
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0",
    "react-drip-form": ">=0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^1.0.0",
    "jest": "^20.0.4",
    "jest-styled-components": "^4.0.1",
    "material-ui": "^0.18.6",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-drip-form": "^0.0.1-alpha.7",
    "react-hot-loader": "next",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "prop-types": "^15.5.10"
  },
  "jest": {
    "rootDir": "src",
    "testMatch": [
      "**/?(*.)spec.js"
    ]
  }
}
